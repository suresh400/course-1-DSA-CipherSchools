//  Array passing
#include<iostream>
using namespace std;

int main(){
    int a[10];
    cout<<"values for camparison : "<<endl;
    
    cout<<"THe value of a  is "<<a<<endl;
    cout<<"The value of address a[0] is "<<&a[0]<<endl;
    cout<<"The value of address a[1] is "<<&a[1]<<endl;
}

